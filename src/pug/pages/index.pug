extends ../layout/main

block variables
	- var title = 'Content';
	- 
		var pagelist = [
			{href: 'menu.html',name:'Menu'},
			{href: 'menu-filter.html',name:'Menu with open filter'},
			{href: 'branch-profile.html',name:'Branch profile'},
			{href: 'restaurant-hours.html',name:'Branch profile (restaurant hours)'},
			{href: 'more-photos.html',name:'More photos'},
			{href: 'tipografika.html',name:'Tipografika'},
		];

block content
	section.container
		h1 Content
		ul
			each page in pagelist
				li
					a(href=page.href,)= page.name


